<script src="https://cdnjs.cloudflare.com/ajax/libs/mermaid/11.5.0/mermaid.min.js" defer></script>
<script>
    document.addEventListener("DOMContentLoaded", function() {
        // Highlight all code blocks except Mermaid
        document.querySelectorAll("pre code:not(.language-mermaid)").forEach((block) => {
            hljs.highlightElement(block);
        });

        // Initialize Mermaid after Highlight.js
        mermaid.initialize({
            startOnLoad: true,
            theme: 'neutral',
            flowchart: {
                useMaxWidth: false,
                htmlLabels: true
            }
        });
        mermaid.init(undefined, document.querySelectorAll('.language-mermaid'));
    });
</script>

